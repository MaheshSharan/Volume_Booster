{
  "manifest_version": 3,
  "name": "Tab Volume Booster",
  "version": "1.0",
  "description": "Overclock and boost the volume of the current tab.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup/popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}